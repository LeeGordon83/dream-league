@model IEnumerable<DreamLeague.Models.Fixture>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AwayManager.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cup.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GameWeek.GameWeekId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HomeManager.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Round)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AwayManager.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cup.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GameWeek.GameWeekId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HomeManager.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Round)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FixtureId }) |
            @Html.ActionLink("Details", "Details", new { id=item.FixtureId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FixtureId })
        </td>
    </tr>
}

</table>
